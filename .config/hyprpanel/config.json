{
  "bar.layouts": {
    "*": {
      "left": [
        "dashboard",
        "workspaces",
        "media"
      ],
      "middle": [
        "windowtitle",
        "clock",
        "notifications"
      ],
      "right": [
        "volume",
        "network",
        "bluetooth",
        "battery",
        "systray",
        "updates"
      ]
    }
  },
  "theme.bar.location": "top",
  "theme.bar.transparent": true,
  "notifications.position": "top",
  "theme.notification.enableShadow": true,
  "notifications.showActionsOnHover": false,
  "menus.clock.time.hideSeconds": true,
